{
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier":
      [
        "error",
        {
          "singleQuote": true,
          "semi": true,
          "printWidth": 100,
          "endOfLine": "auto"
        }
      ],
      "no-unused-expressions": "off",
      "@typescript-eslint/no-unused-expressions": 2
    } 
  }
  